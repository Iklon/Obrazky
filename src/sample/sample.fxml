<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Canvas fx:id="canvas" height="200.0" onMouseClicked="#canvasClicked" width="200.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <VBox alignment="CENTER" spacing="5.0">
               <children>
                  <Button fx:id="button" mnemonicParsing="false" onAction="#loadDir" text="Načti složku" />
                  <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#nextImg" text="Potvrdit" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" text="Název" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
